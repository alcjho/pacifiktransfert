{
  "kind": "collectionType",
  "collectionName": "user_transferts",
  "info": {
    "singularName": "user-transfert",
    "pluralName": "user-transferts",
    "displayName": "user_transfert",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "cdate": {
      "type": "datetime",
      "unique": true,
      "private": true
    },
    "amount_to_send": {
      "type": "decimal",
      "required": true
    },
    "amount_to_receive": {
      "type": "decimal",
      "required": true
    },
    "to_name": {
      "type": "string",
      "required": true
    },
    "to_firstname": {
      "type": "string",
      "required": true
    },
    "to_email": {
      "type": "email",
      "required": false
    },
    "reception_proof": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "user_transferts"
    },
    "to_city": {
      "type": "string"
    },
    "to_address": {
      "type": "string"
    },
    "to_phone": {
      "type": "string"
    },
    "reason": {
      "type": "string"
    },
    "transfert_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::transfert-type.transfert-type"
    },
    "to_other_phone": {
      "type": "string"
    },
    "to_middle_name": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "En traitement",
        "Completer",
        "Action requise",
        "Annuler"
      ],
      "default": "En traitement"
    },
    "message": {
      "type": "text",
      "maxLength": 300,
      "minLength": 20
    },
    "code": {
      "type": "string"
    },
    "bank_branch": {
      "type": "string"
    },
    "account_number": {
      "type": "string"
    },
    "agency_code": {
      "type": "string"
    },
    "agency_key": {
      "type": "string"
    },
    "transfert_bank": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::bank.bank"
    },
    "mobile_money_number": {
      "type": "string"
    }
  }
}
